# Environment variables for TMDB API
# Copy this file to .env and fill in your API credentials

# TMDB API Authentication
# You need either an API key OR an API token (read access token)
TMDB_API_KEY=your_api_key_here
TMDB_API_TOKEN=your_api_read_access_token_here

# Optional: Specify which authentication method to use (key or token)
# If not specified, the code will try API_KEY first, then API_TOKEN
AUTH_METHOD=key


# Docker env for jupyter
JUPYTER_TOKEN=spark1234
JUPYTER_PASSWORD=spark1234